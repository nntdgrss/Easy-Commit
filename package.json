{
  "name": "@nntdgrss/easycommit",
  "version": "1.2.0",
  "description": "Утилита для быстрого создания осмысленных коммитов с минимальными усилиями",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "easycommit": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target bun",
    "prepublishOnly": "bun run build",
    "start": "bun run ./src/index.ts"
  },
  "keywords": [
    "git",
    "commit",
    "cli",
    "automation",
    "productivity"
  ],
  "author": "Nntdgrss",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "bun-types": "latest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nntdgrss/Easy-Commit.git"
  }
}
